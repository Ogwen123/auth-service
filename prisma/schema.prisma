datasource db {
    provider = "postgresql"
    url      = env("URL")
}

generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "linux-arm64-openssl-1.1.x"] // remove when generating on windows
}

model users {
    id            String @id(map: "users_pk") @db.VarChar
    username      String @db.VarChar
    name          String @db.VarChar
    email         String @db.VarChar
    password_hash String @db.VarChar
    perm_flag     Int
    created_at    BigInt
    updated_at    BigInt
}

model blacklisted_tokens {
    token            String @id @db.VarChar(255)
    logout_timestamp Int
}
